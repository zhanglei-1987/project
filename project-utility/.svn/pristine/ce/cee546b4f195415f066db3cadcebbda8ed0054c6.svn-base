package cn.quickly.project.utility.time;

import org.junit.Test;

public class DayOfYearsTest {

	@Test
	public void testIsFirstDay() {

		System.out.println(DayOfYears.isFirstDay("2019-01-01", "yyyy-MM-dd"));

		System.out.println(DayOfYears.isFirstDay("2019-12-31", "yyyy-MM-dd"));

	}

	@Test
	public void testIsLastDay() {

		System.out.println(DayOfYears.isLastDay("2019-01-01", "yyyy-MM-dd"));

		System.out.println(DayOfYears.isLastDay("2019-12-31", "yyyy-MM-dd"));

	}

}
